{
  "name": "@mojaloop/event-sdk",
  "version": "7.4.4-snapshot",
  "description": "Shared code for Event Logging",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "ts_app": "tsc ; ts-node src/examples/ts_app.ts",
    "js_app": "tsc ; ts-node src/examples/js_app.js",
    "server": "tsc ; ts-node src/server/server.ts",
    "test": "npm run test:unit | faucet",
    "pretest": "npm run build",
    "standard": "standard",
    "build": "rm -rf ./lib; tsc; standard --fix",
    "watch": "tsc -w",
    "test:unit": "ts-node node_modules/tape/bin/tape 'test/unit/**/*.test.ts'",
    "test:unit1": "ts-node node_modules/tape/bin/tape ",
    "test:xunit": "npm run test:unit | tap-xunit",
    "test:coverage": "nyc --reporter=lcov --require ts-node/register tape test/unit/**/*.test.{ts,js} | tap-spec; nyc report ---reporter=text",
    "git:stagelib": "git add lib/*.map; git add lib/*.js",
    "git:standardFix": "standard --fix",
    "warn-pre-commit": "echo \"pre commit tasks running. build and test\"",
    "dep:check": "npx ncu -e 2",
    "dep:update": "npx ncu -u"
  },
  "pre-commit": [
    "warn-pre-commit",
    "dep:check",
    "build",
    "git:standardFix",
    "test:unit",
    "git:stagelib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojaloop/event-sdk.git"
  },
  "keywords": [
    "event"
  ],
  "author": "ModusBox",
  "contributors": [
    "Ramiro Gonz√°lez Maciel <ramiro@modusbox.com>",
    "Valentin Genev <valentin.genev@modusbox.com>",
    "Miguel de Barros <miguel.debarros@modusbox.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mojaloop/event-sdk/issues"
  },
  "homepage": "https://github.com/mojaloop/event-sdk#readme",
  "dependencies": {
    "@grpc/proto-loader": "0.5.2",
    "@mojaloop/central-services-shared": "7.5.0",
    "@types/protobufjs": "6.0.0",
    "brototype": "0.0.6",
    "error-callsites": "^2.0.2",
    "grpc": "1.23.3",
    "lodash": "4.17.15",
    "moment": "2.24.0",
    "parse-strings-in-object": "1.2.0",
    "protobufjs": "6.8.8",
    "rc": "1.2.8",
    "serialize-error": "^4.1.0",
    "sinon": "7.4.2",
    "traceparent": "1.0.0",
    "uuid4": "1.1.4"
  },
  "devDependencies": {
    "@types/node": "12.7.4",
    "@types/tape": "4.2.33",
    "@typescript-eslint/eslint-plugin": "2.1.0",
    "@typescript-eslint/parser": "2.1.0",
    "debug": "4.1.1",
    "faucet": "0.0.1",
    "npm-check-updates": "3.1.21",
    "nyc": "14.1.1",
    "pre-commit": "1.2.2",
    "proxyquire": "2.1.3",
    "standard": "14.1.0",
    "tap-spec": "5.0.0",
    "tap-xunit": "2.4.1",
    "tape": "4.11.0",
    "tapes": "4.1.0",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "typedoc": "0.15.0",
    "typescript": "3.3.3333"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "typings"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "standard": {
    "ignore": [
      "/lib"
    ]
  }
}
